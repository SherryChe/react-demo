# 配置参考 https://github.com/facebook/react/blob/master/.editorconfig
# 中文文档参考 http://www.alloyteam.com/2014/12/editor-config/
# 英文文档参考 http://editorconfig.org
root = true

[*]
charset = utf-8
indent_style = space
indent_size = 4
max_line_length = 80
insert_final_newline = true
end_of_line = crlf

# 其他文件设置
[*.md]
max_line_length = 0
